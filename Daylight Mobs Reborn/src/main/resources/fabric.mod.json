{
  "schemaVersion": 1,
  "id": "daylight_mobs_reborn",
  "version": "${version}",
  "name": "Daylight Mobs Reborn",
  "description": "Stops mobs from burning in the daylight (This can be disabled in the config) and allows hostile mobs to spawn during the day (This is disabled by default and can be enabled in the config).",
  "authors": [
    "mcrobertx22 (Original Author)",
    "SmushyTaco (Creator of Fork)"
  ],
  "contact": {
    "homepage": "https://www.smushytaco.com/",
    "issues": "https://www.curseforge.com/minecraft/mc-mods/daylight-mobs-reborn/issues/",
    "sources": "https://github.com/SmushyTaco/Daylight-Mobs-Reborn/"
  },
  "license": "MIT License",
  "icon": "assets/daylight_mobs_reborn/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "com.smushytaco.daylight_mobs_reborn.DaylightMobsReborn"
      }
    ],
    "modmenu": [
      "com.smushytaco.daylight_mobs_reborn.configuration_support.ModMenuIntegration"
    ]
  },
  "mixins": [
    "daylight_mobs_reborn.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.9",
    "fabric-api": "*",
    "fabric-language-kotlin": ">=1.8.2+kotlin.1.7.10",
    "minecraft": "~1.19",
    "cloth-config2": ">=8.0.75",
    "java": ">=17"
  },
  "suggests": {
    "modmenu": ">=4.0.6"
  }
}